    <bpmn:sequenceFlow id="Flow_0duhbem" sourceRef="Gateway_06c3hvb" targetRef="sendSES">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${S(notificationPayload).hasProp('email') == true}</bpmn:conditionExpression>
